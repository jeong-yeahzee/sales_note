{
  "name": "sales_note",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "set NODE_ENV=development&&run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "build": "run-s build:*",
    "build:routify": "routify -b",
    "build:vite": "vite build",
    "build:electron": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.example.sales_note",
    "productName": "sales_note",
    "icon": "icon.png",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "favicon.ico"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.png"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": true
    },
    "directories": {
      "buildResources": "./dist",
      "output": "./build",
      "app": "."
    }
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.8",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "autoprefixer": "^10.4.7",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "svelte": "^3.50.0",
    "svelte-preprocess": "^4.10.7",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "ag-grid-community": "^32.3.3",
    "date-picker-svelte": "^2.15.1",
    "dayjs": "^1.11.13",
    "node-static": "^0.7.11",
    "sqlite3": "^5.1.7"
  }
}
